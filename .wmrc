feh --bg-scale ~/background.jpg &
xrdb ~/.Xresources
xrdb -override ~/.Xresources-override

feh --bg-scale ~/background.jpg
if [ -f ~/.mlocked ]; then
	sleep 0.1
	mlock
fi

export XDG_CONFIG_HOME="$HOME/.config"

xset r rate 200 60
setxkbmap dvorak -option "ctrl:nocaps" -option "altwin:swap_alt_win"
compton -c --config ~/.comptonrc --backend glx --vsync opengl-swc --paint-on-overlay &
scratchpad &

krunner &
gnome-keyring-daemon &
nm-applet &

pkill -9 sxhkd
SHELL=/bin/sh sxhkd -c ~/.sxhkdrc &

sh ~/.startup.sh

synclient RTCornerButton=0
synclient VertEdgeScroll=0
